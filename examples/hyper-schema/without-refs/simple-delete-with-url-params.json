{
  "$schema": "http://json-schema.org/draft-04/hyper-schema",
  "id": "simple-delete-with-url-params",
  "properties": {
    "post_users": {
      "links": [{
        "href": "/users/{id}/{id2}",
        "method": "DELETE",
        "rel": "edit",
        "hrefSchema": {
          "properties": {
            "id": {
              "type": "integer"
            },
            "id2": {
              "type": "integer"
            }
          }
        }
      }]
    }
  }
}
